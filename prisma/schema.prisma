// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Bot_Type {
  button
  dpad
  vr
}

model Bot {
  id   String   @id @unique
  type Bot_Type @default(button)
  ip   String   @default("0.0.0.0")
  port Int      @default(80)
}
